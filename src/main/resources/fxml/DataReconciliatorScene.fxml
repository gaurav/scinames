   <children>
      <TabPane fx:id="upperTabPane" prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="-0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <tabs>
          <Tab text="Reconcile data from one dataset">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Label layoutX="1.0" layoutY="35.0" text="Use names from:" AnchorPane.leftAnchor="1.0" />
                        <ComboBox fx:id="useNamesFromComboBox" layoutX="103.0" layoutY="31.0" prefHeight="27.0" prefWidth="494.0" AnchorPane.leftAnchor="103.0" AnchorPane.rightAnchor="1.0" AnchorPane.topAnchor="31.0" />
                        <Label layoutX="1.0" layoutY="64.0" text="Include data from:" AnchorPane.leftAnchor="1.0" AnchorPane.topAnchor="64.0" />
