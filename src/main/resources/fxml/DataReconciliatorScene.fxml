                        <Label layoutX="1.0" layoutY="6.0" text="Names to use:" AnchorPane.leftAnchor="1.0" AnchorPane.topAnchor="6.0" />
                        <ComboBox fx:id="namesToUseComboBox" layoutX="103.0" layoutY="2.0" prefHeight="27.0" prefWidth="497.0" AnchorPane.leftAnchor="103.0" AnchorPane.rightAnchor="1.0" AnchorPane.topAnchor="2.0" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="Something else">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </Tab>
        </tabs>
      </TabPane>
      <TableView fx:id="dataTableView" prefHeight="222.0" prefWidth="600.0" AnchorPane.bottomAnchor="27.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="151.0">
        <columns>
          <TableColumn prefWidth="75.0" text="C1" />
          <TableColumn prefWidth="75.0" text="C2" />
        </columns>
      </TableView>
      <Button mnemonicParsing="false" onAction="#copyToClipboard" prefHeight="27.0" prefWidth="132.0" text="Copy to Clipboard" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" />
      <Button mnemonicParsing="false" onAction="#exportToCSV" prefHeight="27.0" prefWidth="103.0" text="Export to CSV" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="130.0" />
      <TextField fx:id="statusTextField" editable="false" prefHeight="27.0" prefWidth="365.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="233.0" AnchorPane.rightAnchor="0.0" />
   </children>
</AnchorPane>
