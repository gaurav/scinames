                  <Button layoutX="95.0" layoutY="573.0" mnemonicParsing="false" onAction="#removeDataset" prefHeight="27.0" prefWidth="112.0" text="Remove dataset" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="95.0" />
                  <Button layoutX="786.0" layoutY="573.0" mnemonicParsing="false" onAction="#displayLumpsAndSplits" prefHeight="27.0" prefWidth="111.0" text="Lumps/Splits" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="786.0" />
                  <Button layoutX="897.0" layoutY="573.0" mnemonicParsing="false" onAction="#displayNameStability" prefHeight="27.0" prefWidth="113.0" text="Name stability" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="897.0" />
                  <Button layoutX="619.0" layoutY="573.0" mnemonicParsing="false" onAction="#displayProjectTabularView" prefHeight="27.0" prefWidth="98.0" text="Project stats" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="619.0" />
