<!-- Used to be prefHeight=900, prefWidth=1200 -->

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="1130.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ggvaidya.scinames.ui.ProjectSceneController">
   <children>
